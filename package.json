{
  "name": "base-vaul-monorepo",
  "version": "0.0.1",
  "description": "Drawer component for React made with Base-UI.",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=base-vaul",
    "build:e2e": "turbo run build --filter=e2e",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build lint --filter=base-vaul && changeset version && changeset publish",
    "test:e2e": "turbo run test:e2e --filter=e2e"
  },
  "author": "No√© Tatoud <noe@bearstudio.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntatoud/base-vaul"
  },
  "bugs": {
    "url": "https://github.com/ntatoud/base-vaul/issues"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "prettier": "3.7.4",
    "turbo": "2.6.3",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@10.25.0"
}
