{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test:e2e": {
      "dependsOn": ["^test:e2e"]
    },
    "dev": {
      "cache": false,
      "persistent": false
    }
  }
}
